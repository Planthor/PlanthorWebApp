{
  "name": "planthor-clientfrontendwebapp",
  "version": "0.0.2",
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev",
    "build": "export VERCEL_FORCE_NO_BUILD_CACHE=1 && vite build",
    "build:node": "export SVELTE_ADAPTER=node && vite build",
    "build:netlify": "export SVELTE_ADAPTER=netlify && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --fix --ext .js,.jsx,.cjs .",
    "build_lint": "vite build && eslint .",
    "test": "vitest run --reporter verbose",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "devDependencies": {
    "@neoconfetti/svelte": "^1.0.0",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/adapter-netlify": "^4.3.3",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/adapter-vercel": "^5.1.0",
    "@sveltejs/kit": "^2.8.3",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/svelte": "^5.1.0",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "bits-ui": "^0.21.16",
    "eslint": "^8.44.0",
    "eslint-plugin-svelte": "^2.35.1",
    "jsdom": "^22.1.0",
    "lucide-svelte": "^0.453.0",
    "postcss": "^8.4.38",
    "standard-version": "^9.5.0",
    "svelte": "^4.2.19",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.4.6",
    "vitest": "^1.6.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/montserrat": "^5.0.19",
    "@fontsource/montserrat": "^5.0.18",
    "@internationalized/date": "^3.5.5",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.0",
    "formsnap": "^1.0.1",
    "iconify-icon": "^2.1.0",
    "mode-watcher": "^0.4.1",
    "pkce-gen": "^1.1.0",
    "rollup": "^4.25.0",
    "svelte-sonner": "^0.3.28",
    "sveltekit-superforms": "^2.19.1",
    "tailwind-merge": "^2.4.0",
    "tailwind-variants": "^0.2.1",
    "zod": "^3.23.8"
  }
}
